import java.util.*;

public class Main
{
	public static void main(String[] args) 
	{
	    Scanner s=new Scanner(System.in);
	    int x=s.nextInt();
	    int y=s.nextInt();
	    int b=s.nextInt();
	    boolean bool[]={true,false};
	    if(bool[b%2])
	    System.out.println(y);
	    else
	    System.out.println(x);
	    
	}
}
